<h1>Create New Blog Post</h1>

<%= form_with(model: @tenants_blog_post, local: true) do |form| %>
  <div>
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>

  <div>
    <%= form.label :content %>
    <%= form.text_area :content, rows: 10 %>
  </div>

  <div>
    <%= form.submit 'Create Blog Post' %>
  </div>
<% end %>

<%= link_to 'Back to Blog Posts', tenants_blog_posts_path %>
